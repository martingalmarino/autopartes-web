# Configuración del sitio
NEXT_PUBLIC_SITE_URL=http://localhost:3000
NEXT_PUBLIC_SITE_NAME="Grabado de Autopartes Argentina"

# Token de MapLibre (opcional - para mapas interactivos)
# Obtén tu token gratuito en: https://maplibre.org/maplibre-gl-js-docs/api/
NEXT_PUBLIC_MAPLIBRE_TOKEN=your_maplibre_token_here

# Configuración de Analytics (opcional)
NEXT_PUBLIC_GA_ID=your_google_analytics_id

# Configuración de AdSense (opcional)
NEXT_PUBLIC_ADSENSE_CLIENT=your_adsense_client_id

# Configuración de Vercel (automática en producción)
VERCEL_URL=
VERCEL_ENV=

# Configuración de desarrollo
NODE_ENV=development
